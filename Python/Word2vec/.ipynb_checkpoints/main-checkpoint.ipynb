   "source": [
    "from gensim.models import Doc2Vec\n",
    "\n",
    "model = Doc2Vec(min_count=1, window=10, size=100, sample=1e-4, workers=8)\n",
    "\n",
    "with open('voc.txt','r') as infile:\n",
    "    voc = infile.readlines()\n",
    "    \n",
    "model.build_vocab(voc)"
